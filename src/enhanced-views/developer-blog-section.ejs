<section class="island developer-blog-section">
    <h1>Developer Blog</h1>
    <div class="l-grid l-desktop-grid">
        <% articles.forEach(function (article) { %>
            <div class="l-desktop-grid-cell l-grid-cell">
                <article class="content">
                    <h1 class="h3"><a href="<%= article.webUrl %>"><%= article.webTitle %></a></h1>
                    <time datetime="<%= article.webPublicationDate %>" title="<%= article.webPublicationDate %>"><%= formatDate(article.webPublicationDate) %></time>
                    <p><%- article.fields.trailText %></p>
                </article>
            </div>
        <% }); %>
    </div>
    <p><a href="http://www.theguardian.com/info/developer-blog" class="cta-link">See more</a></p>
</section>
